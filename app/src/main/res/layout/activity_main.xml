        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/enlatados" />

    <Button
        android:id="@+id/oleos"
        android:layout_width="113dp"
        android:layout_height="81dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="70dp"
        android:text="Ã“leos"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/temperos"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/legumes" />

    <Button
        android:id="@+id/embutidos"
        android:layout_width="113dp"
        android:layout_height="81dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="20dp"
        android:text="Embutidos"
        android:textSize="12dp"
        app:layout_constraintBottom_toTopOf="@+id/industria"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/doces"
        app:layout_constraintTop_toBottomOf="@+id/cereais" />

    <Button
        android:id="@+id/industria"
        android:layout_width="113dp"
        android:layout_height="81dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="20dp"
        android:text="Industrializados"
        app:layout_constraintBottom_toTopOf="@+id/leite"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/enlatados"
        app:layout_constraintTop_toBottomOf="@+id/embutidos" />

    <Button
        android:id="@+id/leite"
        android:layout_width="113dp"
        android:layout_height="81dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="20dp"
        android:text="Leite e Derivados"
        android:textSize="13dp"
        app:layout_constraintBottom_toTopOf="@+id/temperos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/legumes"
        app:layout_constraintTop_toBottomOf="@+id/industria" />

    <Button
        android:id="@+id/temperos"
        android:layout_width="113dp"
        android:layout_height="81dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="70dp"
        android:text="Temperos"
        android:textSize="13dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/oleos"
        app:layout_constraintTop_toBottomOf="@+id/leite" />

</androidx.constraintlayout.widget.ConstraintLayout>