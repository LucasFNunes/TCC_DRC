<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.activities.MainActivity">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="68dp"
        android:text="Selecione o tipo do Alimento: "
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/bebidas"
        android:layout_width="113dp"
        android:layout_height="75dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="20dp"
        android:onClick="onClickBebidas"
        android:text="@string/bebidas"
        app:iconTint="#F15A2B"
        app:layout_constraintBottom_toTopOf="@+id/doces"
        app:layout_constraintEnd_toStartOf="@+id/cereais"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:strokeColor="#EDD7D7"
        tools:ignore="UsingOnClickInXml" />

    <Button
        android:id="@+id/cereais"
        android:layout_width="113dp"
        android:layout_height="75dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="20dp"
        android:onClick="onClickCereais"
        android:text="@string/cereais"
        app:layout_constraintBottom_toTopOf="@+id/embutidos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/bebidas"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        tools:ignore="UsingOnClickInXml" />

    <Button
        android:id="@+id/doces"
        android:layout_width="113dp"
        android:layout_height="75dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="20dp"
        android:onClick="onClickDoces"
        android:text="@string/doces"
        app:layout_constraintBottom_toTopOf="@+id/enlatados"
        app:layout_constraintEnd_toStartOf="@+id/embutidos"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bebidas" />

    <Button
        android:id="@+id/enlatados"
        android:layout_width="113dp"
        android:layout_height="75dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="20dp"
        android:onClick="onClickEnlatados"
        android:text="@string/enlatados"
        android:textSize="10sp"
        app:layout_constraintBottom_toTopOf="@+id/legumes"
        app:layout_constraintEnd_toStartOf="@+id/industria"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/doces" />


    <Button
        android:id="@+id/legumes"
        android:layout_width="113dp"
        android:layout_height="75dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="20dp"
        android:onClick="onClickLegumes"
        android:text="@string/leguminosas_sementes"
        android:textSize="9sp"
        app:layout_constraintBottom_toTopOf="@+id/oleos"
        app:layout_constraintEnd_toStartOf="@+id/leite"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/enlatados" />

    <Button
        android:id="@+id/oleos"
        android:layout_width="113dp"
        android:layout_height="75dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="70dp"
        android:onClick="onClickOleos"
        android:text="@string/leos"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/temperos"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/legumes" />

    <Button
        android:id="@+id/embutidos"
        android:layout_width="113dp"
        android:layout_height="75dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="20dp"
        android:onClick="onClickEmbutidos"
        android:text="@string/embutidos"
        android:textSize="10sp"
        app:layout_constraintBottom_toTopOf="@+id/industria"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/doces"
        app:layout_constraintTop_toBottomOf="@+id/cereais" />

    <Button
        android:id="@+id/industria"
        android:layout_width="113dp"
        android:layout_height="75dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="20dp"
        android:onClick="onClickIndustrializados"
        android:text="@string/industrializados"
        android:textSize="10sp"
        app:layout_constraintBottom_toTopOf="@+id/leite"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/enlatados"
        app:layout_constraintTop_toBottomOf="@+id/embutidos" />

    <Button
        android:id="@+id/leite"
        android:layout_width="113dp"
        android:layout_height="75dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="20dp"
        android:onClick="onClickLeite"
        android:text="@string/leite_e_derivados"
        android:textSize="10sp"
        app:layout_constraintBottom_toTopOf="@+id/temperos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/legumes"
        app:layout_constraintTop_toBottomOf="@+id/industria" />

    <Button
        android:id="@+id/temperos"
        android:layout_width="113dp"
        android:layout_height="75dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="70dp"
        android:onClick="onClickTemperos"
        android:text="@string/temperos"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/oleos"
        app:layout_constraintTop_toBottomOf="@+id/leite" />

    <Button
        android:id="@+id/sobre"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:layout_marginStart="202dp"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        android:onClick="onClickSobre"
        android:text="Sobre o App/Ajuda"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/frutas"
        android:layout_width="311dp"
        android:layout_height="55dp"
        android:layout_marginStart="51dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="49dp"
        android:layout_marginBottom="7dp"
        android:onClick="onClickFrutas"
        android:text="Frutas"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/oleos" />

</androidx.constraintlayout.widget.ConstraintLayout>